{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": "other",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
